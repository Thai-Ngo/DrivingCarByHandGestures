<launch>

  <arg name="fixed" default="false"/>
  <arg name="robot_ns" default="/"/>
  <arg name="model_name" default="mycar"/>

  <include file="$(find my_worlds)/launch/spawn_model.launch"
           pass_all_args="true"/>

  <include file="$(find my_worlds)/launch/spawn_controllers.launch">
    <arg name="robot_ns" value="$(arg robot_ns)"/>
  </include>

</launch>
