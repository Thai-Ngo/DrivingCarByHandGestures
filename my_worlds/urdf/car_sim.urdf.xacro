<?xml version="1.0"?>
<robot xmlns:xacro="http://ros.org/wiki/xacro"
       name="car_sim">

  <xacro:arg name="fixed" default="false"/>
  <xacro:arg name="robot_ns" default=""/>

  <xacro:include filename="$(find my_worlds)/urdf/macros.xacro"/>

  <xacro:car_sim robot_ns="$(arg robot_ns)"
                 fixed="$(arg fixed)"/>

</robot>

